CREATE MEMORY TABLE PUBLIC.BOOKINGS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,USER_ID INTEGER NOT NULL,TOUR_ID INTEGER NOT NULL,BOOKING_DATE TIMESTAMP DEFAULT CURRENT_TIMESTAMP,DEPARTURE_DATE DATE,NUM_PEOPLE INTEGER NOT NULL,TOTAL_PRICE DECIMAL(15,2),CONTACT_NAME VARCHAR(100),CONTACT_PHONE VARCHAR(20),CONTACT_EMAIL VARCHAR(100),NOTES CLOB(1G),STATUS VARCHAR(20) DEFAULT 'PENDING',FOREIGN KEY(USER_ID) REFERENCES PUBLIC.USERS(ID),FOREIGN KEY(TOUR_ID) REFERENCES PUBLIC.TOURS(ID))
ALTER TABLE PUBLIC.BOOKINGS ALTER COLUMN ID RESTART WITH 0
