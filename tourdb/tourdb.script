ALTER TABLE PUBLIC.REVIEWS ALTER COLUMN ID RESTART WITH 0
CREATE MEMORY TABLE PUBLIC.PAYMENTS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,BOOKING_ID INTEGER NOT NULL,PAYMENT_METHOD VARCHAR(50),PAYMENT_STATUS VARCHAR(20) DEFAULT 'PENDING',AMOUNT DECIMAL(15,2),PAYMENT_DATE TIMESTAMP,TRANSACTION_ID VARCHAR(100),FOREIGN KEY(BOOKING_ID) REFERENCES PUBLIC.BOOKINGS(ID))
